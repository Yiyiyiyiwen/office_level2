/* pages/tabs/test/special/special.wxss */
.num{
  height: 40rpx;
  width: 40rpx;
  border-radius: 50%;
  text-align: center;
  padding: 5rpx;
  font-size: 30rpx;
  color: white;
  margin-right: 30rpx;
}
.exercise{
  display: flex;
  align-items: center;
  justify-content: space-between;
  margin:0 20rpx;
  padding: 20rpx 0;

}
.content{ 
  width: 100%; 
  height: auto; 
  overflow: hidden; 
  }
.lab{ 
  height: 45rpx;
  width: 45rpx;
  text-align: center;
  border-radius: 50%; 
  color: #fff; 
  font-size: 26rpx;
  margin: 20rpx 0;
  margin-right: 20rpx;
  line-height: 45rpx;
  }
.exer_item{
  display: flex;
  align-items: center;
  border-bottom: 1px #dcdcdc solid;
  position: relative;
}
.exer_name,.exer_count{
  color: #686868;
  font-size: 30rpx;
}
.exer_count{
  position: absolute;
  right: 0;
  font-size: 30rpx;
  margin-right: 10rpx;
}